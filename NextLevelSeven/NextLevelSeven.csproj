	<PropertyGroup>
		<TargetFrameworks>net45;netstandard2.0</TargetFrameworks>
		<PackageId>NextLevelSeven</PackageId>
		<PackageVersion>0.0.2</PackageVersion>
		<Title>NextLevelSeven</Title>
		<Authors>Tony Konzel</Authors>
		<Description>HL7v2 message builder and parser.</Description>
		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<PackageReleaseNotes>See Project URL for usage instructions.</PackageReleaseNotes>
